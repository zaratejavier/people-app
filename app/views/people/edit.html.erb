<h1>Edit Person</h1>

<%= form_for @person do |f|%>
  <%= f.label :first_Name%><br>
  <%= f.text_field :first_name%><br>
  <%= f.label :Last_Name%><br>
  <%= f.text_field :last_name%><br>
  <%= f.label :Age%><br>
  <%= f.text_field :age%><br>
  <%= f.label :Hair_Color%><br>
  <%= f.text_field :hair_color%><br>
  <%= f.label :eye_color%><br>
  <%= f.text_field :eye_color%><br>
  <%= f.label :gender%><br>
  <%= f.text_field :gender%><br>
  <%= f.label :alive%><br>
  <%= f.text_field :alive%><br>
  <%= f.submit%>
<%end%>

