<h1>Edit Person</h1>

<%= form_for @person do |f|%>
  <%= f.label :first_Name%>
  <%= f.text_field :first_name%>
  <%= f.label :Last_Name%>
  <%= f.text_field :last_name%>
  <%= f.label :Age%>
  <%= f.text_field :age%>
  <%= f.label :Hair_Color%>
  <%= f.text_field :hair_color%>
  <%= f.label :eye_color%>
  <%= f.text_field :eye_color%>
  <%= f.label :gender%>
  <%= f.text_field :gender%>
  <%= f.label :alive%>
  <%= f.text_field :alive%>
  <%= f.submit%>
<%end%>

